{
    "name": "puth/laravel",
    "description": "Puth Laravel Client",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Sebastian Uehlin",
            "email": "s.uehlin@proton.me"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "illuminate/log": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "puth/php": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Puth\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puth\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Puth\\Laravel\\PuthServiceProvider"
            ],
            "aliases": {
                "Browser": "Browser\\Browser"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../client",
            "options": {
                "symLink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
